   "source": [
    "import sys \n",
    "import cv2 as cv\n",
    "\n",
    "n=len(sys.argv)\n",
    "if n < 2:\n",
    "    sys.exit(f\"usage: {sys.argv[0]} file\")\n",
    "\n",
    "img_file = sys.argv[1]\n",
    "img = cv.imread(img_file)\n",
    "if img is None:\n",
    "    sys.exit(f\"{img_file} is not an image file!\")\n",
    "\n",
    "cv.imshow(img_file,img)\n",
    "print(\"press any button\")\n",
    "cv.waitKey(0)"
   ]
